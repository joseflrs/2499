+@@ -259,9 +317,6 @@ static int rk_crypto_probe(struct platfo
+ 		goto err_crypto;
+ 	}
+ 
+-	crypto_info->dev = &pdev->dev;
+-	platform_set_drvdata(pdev, crypto_info);
