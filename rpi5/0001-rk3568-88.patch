++	while (sgs && len > 0) {
++		dd = &rkc->tl[ddi];
++
++		todo = min(sg_dma_len(sgs), len);
++		dd->src_addr = sg_dma_address(sgs);
++		dd->src_len = todo;
++		dd->dst_addr = 0;
++		dd->dst_len = 0;
++		dd->dma_ctrl = ddi << 24;
++		dd->iv = 0;
++		dd->next = rkc->t_phy + sizeof(struct rk_crypto_lli) * (ddi + 1);
