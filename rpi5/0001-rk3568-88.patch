++		len -= todo;
++		dd->dma_ctrl |= RK_LLI_DMA_CTRL_SRC_INT;
++		if (len == 0) {
++			dd->user |= RK_LLI_STRING_LAST;
++			dd->dma_ctrl |= RK_LLI_DMA_CTRL_LAST;
++		}
++		dev_dbg(rkc->dev, "HASH SG %d sglen=%d user=%x dma=%x mode=%x len=%d todo=%d phy=%llx\n",
