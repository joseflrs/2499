+ 	phy-mode = "rgmii-id";
+ 	pinctrl-names = "default";
+@@ -259,6 +265,7 @@
+ 		     &gmac1m1_tx_bus2
+ 		     &gmac1m1_rx_bus2
+ 		     &gmac1m1_rgmii_clk
++		     &gmac1m1_clkinout
+ 		     &gmac1m1_rgmii_bus>;
+ 	status = "okay";
+ };
