+@@ -1374,35 +1374,26 @@ static int rockchip_thermal_probe(struct
+ 		return PTR_ERR(thermal->regs);
+ 
+ 	thermal->reset = devm_reset_control_array_get(&pdev->dev, false, false);
+-	if (IS_ERR(thermal->reset)) {
