+ {
+ 	struct rockchip_mbox *mb;
+-	const struct of_device_id *match;
+ 	const struct rockchip_mbox_data *drv_data;
+ 	struct resource *res;
+ 	int ret, irq, i;
+@@ -172,8 +171,7 @@ static int rockchip_mbox_probe(struct pl
+ 	if (!pdev->dev.of_node)
+ 		return -ENODEV;
