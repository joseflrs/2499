++	int ret;
++
++	regmap_update_bits(udphy->pma_regmap, CMN_DP_RSTN_OFFSET,
++			   CMN_DP_CMN_RSTN, FIELD_PREP(CMN_DP_CMN_RSTN, 0x0));
++
++	switch (dp->link_rate) {
++	case 1620:
