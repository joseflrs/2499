++++ b/drivers/thermal/rockchip_thermal.c
+@@ -1350,15 +1350,10 @@ static int rockchip_thermal_probe(struct
+ {
+ 	struct device_node *np = pdev->dev.of_node;
+ 	struct rockchip_thermal_data *thermal;
+-	const struct of_device_id *match;
+ 	int irq;
