++#define RK8602_NVOLTAGES	160
+ 
+ #define TCS_VSEL0_MODE		BIT(7)
+ #define TCS_VSEL1_MODE		BIT(6)
+@@ -64,6 +68,8 @@
+ enum fan53555_vendor {
