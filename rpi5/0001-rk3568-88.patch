++	rk808 = devm_kzalloc(dev, sizeof(*rk808), GFP_KERNEL);
++	if (!rk808)
++		return -ENOMEM;
++	rk808->dev = dev;
++	rk808->variant = variant;
++	rk808->regmap = regmap;
++	dev_set_drvdata(dev, rk808);
++
++	switch (rk808->variant) {
++	case RK805_ID:
++		rk808->regmap_irq_chip = &rk805_irq_chip;
