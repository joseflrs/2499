+-		reg-names = "dbi", "apb", "config";
++		      <0x0 0xf1000000 0x0 0x00100000>,
++		      <0xa 0x40700000 0x0 0x00002000>;
++		reg-names = "dbi", "apb", "config", "atu";
+ 		resets = <&cru SRST_PCIE1_POWER_UP>, <&cru SRST_P_PCIE1>;
