+-
+-	dp-controller {
+-		compatible = "samsung,exynos5-dp";
+-		reg = <0x145b0000 0x10000>;
+-		interrupts = <10 3>;
+-		interrupt-parent = <&combiner>;
+-		clocks = <&clock 342>;
+-		clock-names = "dp";
+-
+-		phys = <&dp_phy>;
+-		phy-names = "dp";
+-	};
