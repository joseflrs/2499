++++ b/arch/arm64/configs/defconfig
+@@ -1274,6 +1274,7 @@ CONFIG_NVMEM_IMX_OCOTP_SCU=y
+ CONFIG_NVMEM_MTK_EFUSE=y
+ CONFIG_NVMEM_QCOM_QFPROM=y
+ CONFIG_NVMEM_ROCKCHIP_EFUSE=y
++CONFIG_NVMEM_ROCKCHIP_OTP=y
