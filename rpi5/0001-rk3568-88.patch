+ 			&substream->pcm->streams[SNDRV_PCM_STREAM_PLAYBACK];
+@@ -1222,6 +1228,12 @@ static int common_soc_init(struct device
+ 	if (trcm == TRCM_TXRX)
+ 		return 0;
+ 
++	if (IS_ERR_OR_NULL(i2s_tdm->grf)) {
