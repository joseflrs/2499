++	algt->stat_req++;
++	rkc->nreq++;
++
++	rctx->mode = algt->rk_mode;
++	rctx->mode |= 0xffff0000;
++	rctx->mode |= RK_CRYPTO_ENABLE | RK_CRYPTO_HW_PAD;
++	writel(rctx->mode, rkc->reg + RK_CRYPTO_HASH_CTL);
